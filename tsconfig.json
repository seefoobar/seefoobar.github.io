{
  "compilerOptions": {
    "target": "ES5",
    "module": "esnext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "lib": [
      "DOM",
      "ES2015"
    ],
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "declaration": true,
    "removeComments": true,
    "sourceMap": true,
    "newLine": "lf",
    "jsx": "preserve",
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}